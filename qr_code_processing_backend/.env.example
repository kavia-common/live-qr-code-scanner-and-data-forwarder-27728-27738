# External API endpoint to receive decoded QR data
EXTERNAL_API_URL=https://httpbin.org/post

# Optional API key for external API (sent as Bearer token)
EXTERNAL_API_KEY=

# Webhook URL to forward the external API response
WEBHOOK_URL=https://httpbin.org/post

# Webcam index (0 is default camera)
CAMERA_INDEX=0

# Milliseconds between frame scans (50..5000)
SCAN_INTERVAL_MS=250

# Avoid sending identical consecutive payloads from scanner
SEND_UNIQUE_ONLY=true

# Comma-separated list of allowed origins for CORS
CORS_ALLOW_ORIGINS=*
